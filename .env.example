OPENROUTER_API_KEY=sk-or-v1-your-openrouter-key
CLADO_API_KEY=your_clado_api_key
AVIATO_API_KEY=your_aviato_api_key
AVIATO_BASE_URL=https://data.api.aviato.co

# Analytics
AMPLITUDE_API_KEY=your_amplitude_api_key

# Model Configuration
MODEL=gpt-4.1-mini
